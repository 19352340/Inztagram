<h1>Edit caption #<%= @photo.id %></h1>
<form action="/update_photo/<%= @photo.id %>">
  <label for="image_url">Image URL</label>
  <input type="text" id="image_url" name="image_url" value="<%= @photo.image_url %>">
  <label for="caption">Caption</label>
  <input type="text" id="caption" name="caption" value="<%= @photo.caption %>">
  <input type="submit" >
</form>
